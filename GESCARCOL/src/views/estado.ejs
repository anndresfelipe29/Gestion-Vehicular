    <body>

		<div class="color-plate ">
			<a class="icon"><i class="fa fa-cog fa-spin"></i></a>
			<h2>Colores</h2>
			<div class="color-head">
				<span class="color1"></span>
				<span class="color2"></span>
				<span class="color3"></span>
				<span class="color4"></span>
				<span class="color5"></span>
				<span class="color6"></span>
				<span class="color7"></span>
				<span class="color8"></span>
				<span class="color9"></span>
			</div>
		</div>

		<!-- Header Area -->
		<header id="header" class="header">
			<!-- Header Inner -->
			<div class="header-inner">
				<div class="container">
					<div class="row">
						<div class="col-md-3 col-sm-2 col-xs-2">
							<div class="logo">
								<a href="/"><img src="img/logo1.png" alt="logo1.png"></a>
							</div>
						</div>
						<div class="col-md-9 col-sm-10">
							<div class="mobile-menu"></div>
							<nav class="navbar navbar-default">
								<div class="collapse navbar-collapse">
									<ul id="nav" class="nav navbar-nav">
										<li class="current"><a href="/">Inicio</a>
										</li>
										<li><a href="#Services">Servicios</a></li>
										<li><a href="#latest-works">Vehiculo<i class="fa fa-caret-down" aria-hidden="true"></i></a>
											<ul class="dropdown">
												<li><a href="/portafolio">Agregar</a></li>
												<li><a href="/estado">Estado</a></li>
											</ul>
										</li>
									</ul>
								</div>
							</nav>
						</div>
					</div>
				</div>
			</div>
		</header>
		<!--/ End Header Area -->


		<!--breadcrumb -->
		<div class="breadcrumb" data-stellar-background-ratio="0.5">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						<div class="breadcrumb-text">
							<h2>Estado Vehículo</h2>
							<ul>
								<li><a href="/">Inicio</a> <i class="fa fa-long-arrow-right" aria-hidden="true"></i></li>
								<li><a href="/estado">Estado</a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--/ End breadcrumb -->
    <!-- features Area -->
		<section id="features" class="section">
			<div class="container">
        <div class="row">
					<div class="col-md-12">
						<div class="section-title">
							<h2>Búsqueda de Vehículo</h2>
						</div>
					</div>
				</div>
        <form action="/vehiculo" method="post">
          <div class="form-group">
          <div class="input-group">
          <input type="text" class="form-control" name="idplaca" id="idplaca" placeholder="Placa Vehiculo">
          <span class="input-group-btn">
          <button type="submit" class="btn btn-default">Buscar</button>
          </span>
          </div>
          </div>
        </form>
				<div class="row">
          <div class="col-md-4 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay="0.9s">
						<div class="feature-right">
							<div class="single-right">
								<i class="fa fa-vcard-o" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Placa</h2>
                  <h4><%=data.PLACA%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-car" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Marca</h2>
									<h4><%=data.MARCA%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-hourglass-2" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Modelo</h2>
									<h4><%=data.MODELO%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-adjust" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Color</h2>
									<h4><%=data.COLOR%></h4>
								</div>
							</div>
						</div>
					</div>
          <div class="col-md-4 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay="0.9s">
						<div class="feature-right">
							<div class="single-right">
								<i class="fa fa-bus" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Tipo</h2>
									<h4><%=data.NOMBRE_TIPO_VEH%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-road" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Kms</h2>
									<h4><%=data.KMS%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-tint" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Combustible</h2>
									<h4><%=data.NOMBRE_COMBU%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-user " aria-hidden="true"></i>
								<div class="right-content">
									<h2>Cliente</h2>
                  <h4><%=data.NOMBRE%></h4>
									</div>
							</div>
						</div>
					</div>
					<div class="col-md-4 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay="0.9s">
						<div class="feature-right">
							<div class="single-right">
								<i class="fa fa-phone-square" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Telefono</h2>
									<h4><%=data.TELEFONO%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-id-card" aria-hidden="true"></i>
								<div class="right-content">
									<h2>C.C. o NIT</h2>
									<h4><%=data.CEDULA%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-address-book" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Dirección</h2>
									<h4><%=data.DIRECCION%></h4>
								</div>
							</div>
							<div class="single-right">
								<i class="fa fa-phone-square" aria-hidden="true"></i>
								<div class="right-content">
									<h2>Celular</h2>
									<h4><%=data.CELULAR%></h4>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<!--/ End features Area -->
