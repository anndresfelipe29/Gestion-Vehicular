		<!-- Latest Works Area -->
		<section id="latest-works" class="latest-works section">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						<div class="section-title">
							<h2>Registro de Vehículo</h2>
							<p>En el siguiente formulario se realiza el registro del vehiculo con el estado de entrega del mismo al taller. </p>
						</div>
					</div>
				</div>
				<div class="row">
					<form action="/search" method="post">
            <fieldset class="form-group">
            <legend>Cliente</legend>
            <div class="form-group">
            <label for="nombre">Documento</label>
            <div class="input-group">
            <input type="text" class="form-control" name="cc" id="cc" placeholder="Documento">
            <span class="input-group-btn">
            <button type="submit" class="btn btn-default">Buscar</button>
            </span>
            </div>
            </div>
            </fieldset>
          </form>
          <form action="/add" method="post">
            <div class="form-group">
						<label for="nombre">Nombre</label>
						<input type="text" class="form-control" name="nombre" id="nombre" aria-describedby="nombreid" placeholder="Nombre" disabled value="<%= data5.NOMBRE%><%= data5.APELLIDO%>">
						<small id="nombreid" class="form-text text-muted">Cliente.</small>
					  </div>
            <div class="form-group">
						<label for="tel">Teléfono</label>
						<input type="text" class="form-control" name="Tel" id="tel" aria-describedby="telid" placeholder="Tel" disabled value="<%= data5.TELEFONO%>">
						<small id="telid" class="form-text text-muted">Télefono fijo del cliente.</small>
					  </div>
            <div class="form-group">
						<label for="direccion">Dirección</label>
						<input type="text" class="form-control" name="direccion" id="direccion" aria-describedby="direccionid" placeholder="Dirección" disabled value="<%= data5.DIRECCION%>">
						<small id="direccionid" class="form-text text-muted">Dirección de residencia.</small>
					  </div>
            <div class="form-group">
						<label for="cel">Celular</label>
						<input type="text" class="form-control" name="cel" id="cel" aria-describedby="celid" placeholder="Cel" disabled value="<%= data5.CELULAR%>">
						<small id="celid" class="form-text text-muted">Celular del cliente.</small>
					  </div>
          <!--</form>
          <form>-->
					  <div class="form-group">
						<label for="placa">Placa</label>
						<input type="text" class="form-control" name="placa" id="placa" aria-describedby="placaid" placeholder="Placa">
						<small id="placaid" class="form-text text-muted">Recuerde que la placa es única.</small>
					  </div>
					  <div class="form-group">
						<label for="marca">Marca</label>
						<input type="text" class="form-control" name="marca" id="marca" aria-describedby="marcaid" placeholder="Marca">
						<small id="marcaid" class="form-text text-muted">Marca del vehiculo.</small>
					  </div>
					  <div class="form-group">
						<label for="modelo">Modelo</label>
						<input type="text" class="form-control" name="modelo" id="modelo" aria-describedby="modeloid" placeholder="Modelo">
						<small id="modeloid" class="form-text text-muted">Año de matricula del vehiculo.</small>
					  </div>
					  <div class="form-group">
						<label for="color">Color</label>
						<input type="text" class="form-control" name="color" id="color" aria-describedby="colorid" placeholder="Color">
						<small id="colorid" class="form-text text-muted">Color visible del vehiculo.</small>
					  </div>
					  <div class="form-group">
						<label for="exampleSelect1">Tipo</label>
						<select class="form-control" name="tipo" id="tipo">
              <% if (data3) { %>
                <% for(var i = 0; i < data3.length; i++) { %>
    						  <option><%= data3[i].ID_TIPO_VEH%>.<%= data3[i].NOMBRE_TIPO_VEH %></option>
                <% } %>
              <% } %>
						</select>
					  </div>
					  <div class="form-group">
						<label for="color">Kilometros</label>
						<input type="text" class="form-control" name="kms" id="kms" aria-describedby="kmsid" placeholder="Kms">
						<small id="kmsid" class="form-text text-muted">Kilometros registrados por el vehiculo.</small>
					  </div>
					  <div class="form-group">
						<label for="combustible">Tipo de Combustible</label>
						<select class="form-control" name="combustible" id="combustible">
              <% if (data4) { %>
                <% for(var i = 0; i < data4.length; i++) { %>
    						  <option><%= data4[i].ID_COMBUSTIBLE %>.<%= data4[i].NOMBRE_COMBU %></option>
                <% } %>
              <% } %>
						</select>
					  </div>
					  <!--<fieldset class="form-group">
						<legend>Items Vehículo</legend>
            <% if (data1) { %>
              <% for(var i = 0; i < data1.length; i++) { %>
    						<div class="form-check">
    						  <label class="form-check-label">
    							<input type="checkbox" class="form-check-input" name="item<%= data1[i].ID_ITEM %>" id="item<%= data1[i].ID_ITEM %>" value="<%= data1[i].ID_ITEM %>">
    							<%= data1[i].NOMBRE_ITEM %>
    						  </label>
    						</div>
              <% } %>
            <% } %>
          </fieldset>-->
            <div class="form-group">
						<label for="combustible">Items Vehículo</label>
						<select multiple class="form-control" name="item" id="item">
              <% if (data1) { %>
                <% for(var i = 0; i < data1.length; i++) { %>
    						  <option><%= data1[i].ID_ITEM %>.<%= data1[i].NOMBRE_ITEM %></option>
                <% } %>
              <% } %>
						</select>
					  </div>
            <div class="form-group">
						<label for="combustible">Luces Vehículo</label>
						<select multiple class="form-control" name="luz" id="luz">
              <% if (data2) { %>
                <% for(var i = 0; i < data2.length; i++) { %>
    						  <option><%= data2[i].ID_LUZ_INDICADOR %>.<%= data2[i].NOMBRE_LUZ %></option>
                <% } %>
              <% } %>
						</select>
					  </div>
            <!--<fieldset class="form-group">
						<legend>Luces Vehículo</legend>
            <% if (data2) { %>
              <% for(var i = 0; i < data2.length; i++) { %>
    						<div class="form-check">
    						  <label class="form-check-label">
    							<input type="checkbox" class="form-check-input" name="luz<%= data2[i].ID_LUZ_INDICADOR %>" id="luz<%= data2[i].ID_LUZ_INDICADOR %>" value="<%= data2[i].ID_LUZ_INDICADOR %>">
    							<%= data2[i].NOMBRE_LUZ %>
    						  </label>
    						</div>
              <% } %>
            <% } %>
          </fieldset>-->
            <div class="form-group">
						<label for="observación">Observación</label>
						<textarea class="form-control" name="observacion" id="observacion" rows="3"></textarea>
					  </div>
					  <button type="submit" class="btn btn-primary">Agregar</button>
					</form>
